<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="5" name="Suite" parallel="tests">

	<listeners>
	
		<listener class-name="com.utilities.OC_ExtentReportManager"></listener>
	
	</listeners>

	<test name="chrome_test">

		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="chrome"></parameter>

		<classes>
			<class name="com.testcases.TC001_AccountRegistrationTest" />
		</classes>
	</test> <!--
	Test Chrome Browser-->

	<test name="edge_test">

		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="edge"></parameter>

		<classes>
			<class name="com.testcases.TC001_AccountRegistrationTest" />
		</classes>
	</test> <!--
	Test Edge Browser -->

	<test name="firefox_test">

		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="firefox"></parameter>

		<classes>
			<class name="com.testcases.TC001_AccountRegistrationTest" />
		</classes>
	</test> <!--
	Test FrireFox Browser -->

</suite> <!-- Suite -->
